{
    "manifests": {
        "friendsofphp/php-cs-fixer": {
            "manifest": {
                "copy-from-recipe": {
                    ".php_cs.dist": ".php_cs.dist"
                },
                "gitignore": [
                    "/.php_cs",
                    "/.php_cs.cache"
                ]
            },
            "files": {
                ".php_cs.dist": {
                    "contents": [
                        "<?php",
                        "",
                        "$finder = PhpCsFixer\\Finder::create()",
                        "    ->in(__DIR__)",
                        "    ->exclude('var')",
                        ";",
                        "",
                        "return PhpCsFixer\\Config::create()",
                        "    ->setRules([",
                        "        '@Symfony' => true,",
                        "        'array_syntax' => ['syntax' => 'short'],",
                        "    ])",
                        "    ->setFinder($finder)",
                        ";",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "cc05ab6abf6894bddb9bbd6a252459010ebe040b"
        }
    }
}
