{
    "bundles": {
        "Doctrine\\Bundle\\MongoDBBundle\\DoctrineMongoDBBundle": ["all"]
    },
    "copy-from-recipe": {
        "config/": "%CONFIG_DIR%/",
        "src/": "%SRC_DIR%/"
    },
    "env": {
        "MONGODB_URI": "mongodb://localhost:27017",
        "MONGODB_DB": "symfony"
    },
    "conflict": {
        "doctrine/cache": "<1.11",
        "symfony/framework-bundle": "<5.3"
    },
    "aliases": ["mongodb-odm", "doctrine-mongodb", "mongo-odm", "doctrine-mongo"]
}
